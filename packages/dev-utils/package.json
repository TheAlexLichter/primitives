{
  "name": "@netlify/dev-utils",
  "version": "1.0.0",
  "description": "JavaScript utilities for local development",
  "type": "module",
  "engines": {
    "node": "^14.16.0 || >=16.0.0"
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/main.d.cts",
        "default": "./dist/main.cjs"
      },
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      },
      "default": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "server.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "prepack": "npm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "npm run build && vitest run",
    "dev": "tsc --watch"
  },
  "keywords": [],
  "license": "MIT",
  "repository": "netlify/dev-utils",
  "bugs": {
    "url": "https://github.com/netlify/dev-utils/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "tmp-promise": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.0.0",
    "vitest": "^0.34.0"
  },
  "dependencies": {
    "chokidar": "^4.0.1",
    "decache": "^4.6.2",
    "lodash.debounce": "^4.0.8"
  }
}
